{
    "name": "molay76/laravel-boilerplate",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "8.1.*",
        "ext-gd": "*",
        "ext-json": "*",
        "ext-pdo": "*",
        "ext-mbstring": "*",
        "ext-zip": "*",
        "fideloper/proxy": "^4.4",
        "jdorn/sql-formatter": "^1.2",
        "kreait/firebase-php": "^5.14",
        "laravel/framework": "8.*",
        "laravel/tinker": "^2.4",
        "maatwebsite/excel": "^3.1",
        "setasign/fpdi": "^2.3",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "stichoza/google-translate-php": "^4.1",
        "symfony/yaml": "^5.2",
        "tecnickcom/tcpdf": "^6.4",
        "thedoctor0/laravel-factory-generator": "^1.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.7",
        "fakerphp/faker": "^1.13",
        "friendsofphp/php-cs-fixer": "^2.16",
        "greenlion/php-sql-parser": "^4.4",
        "infyomlabs/laravel-generator": "8.0.x-dev",
        "itsgoingd/clockwork": "^5.0",
        "laravel/dusk": "^6.7",
        "mockery/mockery": "^1.4",
        "molay76/laravel-mysql2plantuml": "^1.9",
        "phpmetrics/phpmetrics": "^2.7",
        "phpunit/phpunit": "^9.3",
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.5.8",
        "ucan-lab/laravel-dacapo": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeders",
            "database/factories"
        ],
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {}
}
